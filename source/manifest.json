{
	"name": "Yealink Click2Dial",
	"version": "0.0.0",
	"description": "An awesome new browser extension",
	"homepage_url": "https://github.com/optimise-group/click2dial-for-yealink",
	"manifest_version": 2,
	"minimum_chrome_version": "74",
	"applications": {
		"gecko": {
			"strict_min_version": "67.0"
		}
	},
	"icons": {
		"128": "icon.png"
	},
	"permissions": [
		"storage"
	],
	"options_ui": {
		"chrome_style": true,
		"page": "options.html"
	},
	"background": {
		"persistent": false,
		"scripts": [
			"background.js"
		]
	}
}
